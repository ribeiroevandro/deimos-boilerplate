{
  "name": "deimos-boilerplate",
  "version": "0.0.1",
  "description": "A simple and fast boilerplate for static projects using Gulp, Pug, Stylus and PostCSS",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ribeiroevandro/deimos-boilerplate.git"
  },
  "browserslist": [
    "> 5%",
    "last 2 versions"
  ],
  "scripts": {
    "start": "gulp",
    "test": "npm run lint && npm run reporter",
    "setup": "npm i && gulp build",
    "fix": "./node_modules/.bin/eslint ./src/ --fix",
    "lint": "npm run lint:js",
    "lint:js": "./node_modules/.bin/eslint ./src/",
    "reporter": "./node_modules/.bin/parker ./build/assets/css/*.css",
    "precommit": "npm run lint",
    "build": "gulp build",
    "html": "gulp html",
    "js": "gulp js",
    "css": "gulp css",
    "images": "gulp images",
    "icons": "gulp icons",
    "svg": "gulp svg",
    "surge": "gulp surge"
  },
  "keywords": [
    "boilerplate"
  ],
  "author": "Evandro Ribeiro <ribeiroevandro@live.com> (http://ribeiroevandro.com.br)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ribeiroevandro/deimos-boilerplate/issues"
  },
  "homepage": "https://github.com/ribeiroevandro/deimos-boilerplate#readme",
  "devDependencies": {
    "autoprefixer": "^7.1.5",
    "babel": "^6.23.0",
    "babel-core": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-es2015-rollup": "^3.0.0",
    "browser-sync": "^2.18.13",
    "del": "^3.0.0",
    "eslint": "^4.8.0",
    "gulp": "^3.9.1",
    "gulp-babel": "^7.0.0",
    "gulp-cheerio": "^0.6.2",
    "gulp-concat": "^2.6.1",
    "gulp-cssnano": "^2.1.2",
    "gulp-data": "^1.2.1",
    "gulp-group-css-media-queries": "^1.2.0",
    "gulp-imagemin": "^3.4.0",
    "gulp-plumber": "^1.1.0",
    "gulp-postcss": "^7.0.0",
    "gulp-pug": "^3.3.0",
    "gulp-sourcemaps": "^2.6.1",
    "gulp-stylus": "^2.6.0",
    "gulp-surge": "^0.1.0",
    "gulp-svgmin": "^1.2.4",
    "gulp-svgstore": "^6.1.0",
    "gulp-uglify": "^3.0.0",
    "husky": "^0.14.3",
    "js-yaml": "^3.13.1",
    "lost": "^8.2.0",
    "mdcss": "^1.5.2",
    "nib": "^1.1.2",
    "parker": "^0.0.10",
    "postcss-flexibility": "^2.0.0",
    "postcss-font-magician": "^2.1.0",
    "poststylus": "^1.0.0",
    "rucksack-css": "^1.0.2",
    "rupture": "^0.6.2"
  }
}
