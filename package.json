{
  "name": "deimos-boilerplate",
  "version": "0.0.1",
  "description": "A simple and fast boilerplate for static projects using Gulp, Pug, Stylus and PostCSS",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ribeiroevandro/deimos-boilerplate.git"
  },
  "scripts": {
    "lint": "./node_modules/.bin/eslint src",
    "fix": "./node_modules/.bin/eslint src --fix",
    "reporter": "./node_modules/.bin/parker ./build/css/",
    "precommit": "npm run lint",
    "prepush": "npm run lint"
  },
  "keywords": [
    "boilerplate"
  ],
  "author": "Evandro Ribeiro <ribeiroevandro@live.com> (http://ribeiroevandro.com.br)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ribeiroevandro/deimos-boilerplate/issues"
  },
  "homepage": "https://github.com/ribeiroevandro/deimos-boilerplate#readme",
  "devDependencies": {
    "babel": "^6.5.2",
    "babel-core": "^6.7.4",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-es2015-rollup": "^1.1.1",
    "browser-sync": "^2.12.3",
    "eslint": "^2.10.0",
    "gulp": "^3.9.1",
    "gulp-cheerio": "^0.6.2",
    "gulp-concat": "^2.6.0",
    "gulp-cssnano": "^2.1.1",
    "gulp-data": "^1.2.1",
    "gulp-gh-pages": "^0.5.4",
    "gulp-group-css-media-queries": "^1.1.0",
    "gulp-imagemin": "^3.0.1",
    "gulp-plumber": "^1.1.0",
    "gulp-postcss": "^6.1.0",
    "gulp-pug": "^3.0.3",
    "gulp-sourcemaps": "^1.6.0",
    "gulp-stylus": "^2.3.1",
    "gulp-svgmin": "^1.2.2",
    "gulp-svgstore": "^6.0.0",
    "gulp-uglify": "^1.5.3",
    "husky": "^0.11.4",
    "js-yaml": "^3.6.1",
    "lost": "^6.7.2",
    "parker": "0.0.10",
    "postcss-font-magician": "^1.4.0",
    "poststylus": "^0.2.3",
    "rollup-plugin-babel": "^2.4.0",
    "rollup-plugin-commonjs": "^2.2.1",
    "rollup-plugin-node-resolve": "^1.5.0",
    "rucksack-css": "^0.8.5",
    "rupture": "^0.6.1"
  }
}
